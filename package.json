{
	"name": "canix",
	"version": "0.0.1",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"android": "react-native run-android",
		"ios:dev": "ENVFILE=.env.dev react-native run-ios",
		"ios:device": "ENVFILE=.env.dev react-native run-ios --device",
		"ios:prod": "ENVFILE=.env.prod react-native run-ios --configuration Release",
		"start": "react-native start",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"clean": "rm -rf node_modules ios/Podfile.lock ios/build ios/Pods ~/Library/Developer/Xcode/DerivedData",
		"deps_ios": "yarn && cd ios && USE_HERMES=1 pod install",
		"new-arch": "cd ios && USE_HERMES=1 USE_FABRIC=1 RCT_NEW_ARCH_ENABLED=1 pod install",
		"stop": "kill -9 $(lsof -t -i:8081)"
	},
